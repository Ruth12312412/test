# AI后台管理系统环境变量配置示例
# 复制此文件为 .env 并填入真实的配置值

# 数据库配置
DB_HOST=mysql
DB_PORT=3306
DB_NAME=ai_admin_system
DB_USERNAME=ai_admin
DB_PASSWORD=ai_admin123

# Redis配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT配置
JWT_SECRET=ai-admin-system-jwt-secret-key-2024-production

# OpenAI配置
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_BASE_URL=https://api.openai.com/v1

# MidJourney配置
MIDJOURNEY_API_KEY=your-midjourney-api-key-here
MIDJOURNEY_BASE_URL=https://api.midjourney.com/v1

# 文件上传配置
FILE_UPLOAD_PATH=/app/uploads
FILE_MAX_SIZE=52428800

# 应用配置
SPRING_PROFILES_ACTIVE=prod
SERVER_PORT=8080

# 日志配置
LOG_LEVEL=INFO